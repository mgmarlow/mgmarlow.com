---
pagination:
  data: collections
  size: 1
  alias: tag
permalink: /tags/{{ tag }}/
title: "tags - mgmarlow.com"
layout: "layouts/page.njk"
---

<h1>Tagged "{{ tag }}"</h1>

<ol>
  {% set taglist = collections[tag] %}
  {% for post in taglist | reverse %}
    <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
  {% endfor %}
</ol>
