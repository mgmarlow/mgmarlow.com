{
  "name": "@example/starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "yarn build && concurrently \"eleventy --serve --output=out\" \"yarn tw:start -w\"",
    "build": "eleventy --output=out && yarn tw:build",
    "tw:start": "tailwindcss -i index.css -o out/index.css",
    "tw:build": "tailwindcss -i index.css -o out/index.css --minify"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@tailwindcss/typography": "^0.5.1",
    "luxon": "^2.3.0",
    "tailwindcss": "^3.0.1"
  },
  "devDependencies": {
    "concurrently": "^7.1.0"
  }
}
