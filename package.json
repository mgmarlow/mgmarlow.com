{
  "name": "@example/starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx @11ty/eleventy --serve --output=out & yarn tw:start",
    "build": "npx @11ty/eleventy --formats=html --output=out & yarn tw:build",
    "tw:build": "npx tailwindcss -i index.css -o out/index.css --minify",
    "tw:start": "npx tailwindcss -i index.css -o out/index.css --watch"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "tailwindcss": "^3.0.1"
  }
}
